<?xml version="1.0"?>

<robot name="rb1_robot">


    <!-- Materials -->
    <gazebo reference="base_link">
        <visual>
            <material> <!-- Black -->
                <ambient>0.05 0.05 0.05 1.0</ambient>
                <diffuse>0.05 0.05 0.05 1.0</diffuse>
                <specular>0.0 0.0 0.0 1.0</specular>
                <emissive>0.0 0.0 0.0 1.0</emissive>
            </material>
        </visual>
    </gazebo>

    <gazebo reference="link_left_wheel">
        <visual>
            <material> <!-- Pink -->
                <ambient>0.30 0.06 0.18 1.0</ambient>
                <diffuse>1.0 0.20 0.60 1.0</diffuse>
                <specular>1.0 0.60 0.90 1.0</specular>
                <emissive>0.0 0.0 0.0 1.0</emissive>
                <shininess>90</shininess>
            </material>
        </visual>
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>
    <gazebo reference="link_right_wheel">
        <visual>
            <material> <!-- Pink -->
                <ambient>0.30 0.06 0.18 1.0</ambient>
                <diffuse>1.0 0.20 0.60 1.0</diffuse>
                <specular>1.0 0.60 0.90 1.0</specular>
                <emissive>0.0 0.0 0.0 1.0</emissive>
                <shininess>90</shininess>
            </material>
        </visual>
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <gazebo reference="front_caster">
        <visual>
            <material> <!-- Green -->
                <ambient>0.10 0.40 0.10 1.0</ambient>
                <diffuse>0.00 1.00 0.00 1.0</diffuse>
                <specular>0.90 0.90 0.90 1.0</specular>
                <emissive>0.00 0.20 0.00 1.0</emissive>
                <shininess>90</shininess>
            </material>
        </visual>
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <gazebo reference="back_caster">
        <visual>
            <material> <!-- Green -->
                <ambient>0.10 0.40 0.10 1.0</ambient>
                <diffuse>0.00 1.00 0.00 1.0</diffuse>
                <specular>0.90 0.90 0.90 1.0</specular>
                <emissive>0.00 0.20 0.00 1.0</emissive>
                <shininess>90</shininess>
            </material>
        </visual>
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>


    <!-- Define materials for visualization -->
    <material name="blue"><color rgba="0 0 1 1"/></material>
    <material name="gray"><color rgba="0.40 0.40 0.40 1"/></material>
    <material name="red"><color rgba="1 0 0 1"/></material>

    <!-- Base footprint link -->
    <link name="base_footprint">
    </link>

    <!-- Base link -->
    <link name="base_link">
        <inertial>
            <mass value="20.2" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.46710" ixy="0" ixz="0" iyy="0.46710" iyz="0" izz="0.63125" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.30" />
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <!-- Joint connecting base_link and base_footprint -->
    <joint name="base_link_plate_joint" type="fixed">
        <parent link="base_footprint"/>
        <child  link="base_link"/>
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </joint>

    <!-- Joint connecting base_link and front_caster -->
    <joint name="base_link_front_caster_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0 -0.1625" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties friction="0.0" />
    </joint>

    <!-- Front caster -->
    <link name="front_caster">
        <inertial>
            <mass value="0.9" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00032400" ixy="0" ixz="0"
             iyy="0.00032400" iyz="0"
             izz="0.00032400"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0185" />
            <geometry>
                <sphere radius="0.03" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0185" />
            <geometry>
                <sphere radius="0.03" />
            </geometry>
            <material name="gray"/>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0125" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0125" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="gray"/>
        </visual>
    </link>

    <!-- Joint connecting base_link and back_caster -->
    <joint name="base_link_back_caster_joint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0 -0.1625" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties friction="0.0" />
    </joint>

    <!-- Back caster -->
    <link name="back_caster">
        <inertial>
            <mass value="0.9" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00032400" ixy="0" ixz="0"
             iyy="0.00032400" iyz="0"
             izz="0.00032400"/>
        </inertial>

        <!-- Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0185" />
            <geometry>
                <sphere radius="0.03" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0185" />
            <geometry>
                <sphere radius="0.03" />
            </geometry>
            <material name="gray"/>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0125" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0125" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="gray"/>
        </visual>
    </link>



    <!-- Joint - base_link / left wheel -->
    <joint name="joint_base_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <child link="link_left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left wheel -->
    <link name="link_left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00028958" ixy="0" ixz="0"
             iyy="0.00031250" iyz="0"
             izz="0.00028958"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0.07 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0.07 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
            <material name="red"/>
        </visual>

        <!-- Axle -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0.03 0" />
            <geometry>
                <cylinder length="0.06" radius="0.01"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0.03 0" />
            <geometry>
                <cylinder length="0.06" radius="0.01"/>
            </geometry>
            <material name="red"/>
        </visual>
    </link>


    <!-- Joint - base / right wheel -->
    <joint name="joint_base_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <child link="link_right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - right wheel -->
    <link name="link_right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00028958" ixy="0" ixz="0"
             iyy="0.00031250" iyz="0"
             izz="0.00028958"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.07 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 -0.07 0" />
            <geometry>
                <cylinder length="0.04" radius="0.025"/>
            </geometry>
            <material name="red"/>
        </visual>

        <!-- Axle -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.03 0" />
            <geometry>
                <cylinder length="0.06" radius="0.01"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 -0.03 0" />
            <geometry>
                <cylinder length="0.06" radius="0.01"/>
            </geometry>
            <material name="red"/>
        </visual>
    </link>

    <!-- Front laser link -->
    <link name="front_laser">
        <inertial>
            <mass value="1.0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- Box 0.06 x 0.06 x 0.08 m -->
            <inertia ixx="0.00083333" ixy="0" ixz="0"
             iyy="0.00083333" iyz="0"
             izz="0.00060000" />
        </inertial>
    </link>

    <!-- Joint - base / laser -->
    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.14159265 0 0" />
        <child link="front_laser" />
        <parent link="base_link" />
    </joint>


    <!-- Differential Drive Plugin -->
    <gazebo>
        <plugin
            filename="libignition-gazebo-diff-drive-system.so"
            name="ignition::gazebo::systems::DiffDrive">
            <left_joint>joint_base_left_wheel</left_joint>
            <right_joint>joint_base_right_wheel</right_joint>
            <wheel_separation>0.5</wheel_separation>
            <wheel_radius>0.025</wheel_radius>
            <odom_publish_frequency>20</odom_publish_frequency>
            <topic>cmd_vel</topic>
            <odom_topic>odom</odom_topic>
            <tf_topic>tf</tf_topic>
            <frame_id>odom</frame_id>
            <child_frame_id>base_link</child_frame_id>
        </plugin>
    </gazebo>

    <!-- Gazebo tags - Laser scan -->
    <gazebo reference="base_link_front_laser_joint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>

    <!-- Laser Scanner Plugin -->
    <gazebo>
        <plugin
        filename="libignition-gazebo-sensors-system"
        name="ignition::gazebo::systems::Sensors">
        <!-- use ogre2 if ogre v2.x is installed, otherwise use ogre -->
        <render_engine>ogre2</render_engine>
        </plugin>
    </gazebo>
    <gazebo reference="front_laser">
        <sensor type="gpu_lidar" name="head_hokuyo_sensor">
            <pose relative_to='lidar_frame'>0 0 0 0 0 0</pose>
            <always_on>1</always_on>
            <visualize>true</visualize>
            <update_rate>20</update_rate>
            <topic>scan</topic>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.570796</min_angle>
                        <max_angle>1.570796</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.20</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
        </sensor>
    </gazebo>


</robot>